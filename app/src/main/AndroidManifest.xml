<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission
        android:name="android.permission.MANAGE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".ohther.MobileCleanApplication"
        android:requestLegacyExternalStorage="true"
        android:theme="@style/Theme.MobileClean0815">
        <activity
            android:name=".ohther.OneActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTop"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.MainActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTask"
            android:exported="true"/>
        <activity android:name=".ohther.ShowSetActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTop"
            android:exported="true"/>
        <activity android:name=".junk.JunkActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTop"
            android:exported="true"/>
        <activity android:name=".big.BigFileListActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTop"
            android:exported="true"/>
        <activity android:name=".img.ImageListActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTop"
            android:exported="true"/>
        <activity android:name=".finish.FinishActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTop"
            android:exported="true"/>
    </application>

</manifest>